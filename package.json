    "babel-plugin-relay": "1.0.1",
    "eslint-config-prettier": "2.1.1",
    "eslint-plugin-prettier": "2.1.1",
    "husky": "0.13.4",
    "jailbreak-react-scripts": "file:packages/jailbreak-react-scripts",
